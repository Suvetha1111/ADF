							{
								"source": {
									"path": "$['company']['name']"
								},
								"sink": {
									"name": "CompanyName"
								}
							},
							{
								"source": {
									"path": "$['company']['catchPhrase']"
								},
								"sink": {
									"name": "CatchPhrase"
								}
							},
							{
								"source": {
									"path": "$['company']['bs']"
								},
								"sink": {
